{{!-- <h1>Hello World!</h1> --}}

<div class="pageContainer">
  <div class="titleContainer">
   <h1 class="pageTitle">_ARCHITEKT</h1>
   <p class="pageDescription">Welcome to our online portfolio, we are a small group of passionate designers and architects, looking to change and create amazing digital images to inspire others to follow in our innovative steps. Free Architect WordPress Theme</p>
  </div>

  <div class="galleryContainer">
    <div class="mainImage" style="background-image:url({{firstImage.dataValues.url}})"></div>

    {{#each otherImages}}
      <div class="imageContainer">
        <div class="image" style="background-image:url({{dataValues.url}})"></div>
        <div class="imageDetails">
          <p class="imageAuthor">{{Author.dataValues.name}}</p>
          <p class="imageUrl">{{dataValues.url}}</p>
        </div>
      </div>
    {{/each}}
   </div>
</div>